<script>
function round5(x){
return Math.ceil(x/5)*5;
}
$(document).ready(function() {
$("#qnh").change(function() {
var qnh_value = $("#qnh").val();
var mhPa = 8.5344;
var pMer = 1013.25;
var delta = 0;
$("#qnh").val(qnh_value);
delta = (qnh_value - pMer) * mhPa;
delta = delta.toFixed(0);
$("#delta").text(delta);
fl = delta*1 + 2286*1;
fl = round5(fl);
;
$("#fl75").text(fl);
fl = delta*1 + 3505*1;
fl = round5(fl);
;
$("#fl115").text(fl);
fl = delta*1 + 3810*1;
fl = round5(fl);
;
$("#fl125").text(fl);
fl = delta*1 + 4115*1;
fl = round5(fl);
;
$("#fl135").text(fl);
fl = delta*1 + 4419*1;
fl = round5(fl);
;
$("#fl145").text(fl); fl = delta*1 + 4724*1;
fl = round5(fl);
;
$("#fl155").text(fl);
fl = delta*1 + 5943*1;
fl = round5(fl);
fl = fl+' (5800 Bure)';
$("#fl195").text(fl);
});
});
</script></section></section>
